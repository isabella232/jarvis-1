{
  "name": "@wings-software/jarvis",
  "version": "0.2.0",
  "license": "UNLICENSED",
  "files": [
    "lib"
  ],
  "bin": {
    "jest-grouped-coverage": "bin/jest-grouped-coverage.js",
    "jest-text-coverage-reporter": "bin/jest-text-coverage-reporter.js"
  },
  "scripts": {
    "test": "tsc --noEmit",
    "clean": "rm -rf lib",
    "prepublishOnly": "yarn build",
    "build": "yarn clean && tsc && copyfiles -e '**/*.ts' -u 1 'src/**/*.*' lib"
  },
  "peerDependencies": {},
  "dependencies": {
    "ajv": "^6.12.2",
    "better-ajv-errors": "^0.6.7",
    "chalk": "^4.0.0",
    "copyfiles": "^2.3.0",
    "globby": "^11.0.1",
    "handlebars": "^4.7.6",
    "lodash": "^4.17.15",
    "mkdirp": "^1.0.4",
    "tslib": "^2.0.0",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@types/handlebars": "^4.1.0",
    "@types/lodash": "^4.14.155",
    "@types/mkdirp": "^1.0.0",
    "@types/webpack": "^4.41.17",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.7",
    "prettier": "^2.0.5",
    "typescript": "^3.9.3"
  },
  "optionalDependencies": {
    "webpack": "^4.0.0"
  },
  "engines": {
    "node": ">=13.8.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint",
      "sh scripts/typecheck-staged.sh",
      "prettier --check"
    ],
    "*.scss": [
      "prettier --check"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "node ./scripts/lint-commit-message.js \"$(cat $HUSKY_GIT_PARAMS)\""
    }
  }
}
